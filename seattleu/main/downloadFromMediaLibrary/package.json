{
  "name": "downloadfrommedialibrary",
  "version": "1.0.1",
  "description": "",
  "main": "./dist/bundle.js",
  "bin": "./dist/bundle.js",
  "type": "commonjs",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "mkdir -p dist && esbuild index.js --bundle --platform=node --format=cjs --outfile=dist/bundle.js --external:node:* --external:fs --external:path",
    "package": "pkg . --targets node16-win-x64 --output dist/media-downloader.exe"
  },
  "author": "Pierce Harriz",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.7.0",
    "t4.ts": "^0.0.11",
    "tslib": "^2.8.1",
    "zip-a-folder": "^3.1.6"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "promptUI/**/*",
      "t4apiwrapper/**/*",
      "dist/**/*"
    ],
    "scripts": [
      "index.js",
      "../promptUI/**/*.mjs",
      "../promptUI/**/*.js",
      "../../../../t4apiwrapper/**/*.js"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "pkg": "^5.8.1"
  }
}
